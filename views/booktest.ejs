      `;
      cardContainer.appendChild(card);
    });
  }

  function renderPagination(dataSet) {
    const pageCount = Math.ceil(dataSet.length / cardsPerPage);
    pagination.innerHTML = "";

    for (let i = 1; i <= pageCount; i++) {
      const btn = document.createElement("button");
      btn.innerText = i;
      btn.className = `text-lg px-2 py-1 rounded-md border ${
        i === currentPage ? "bg-blue-600 text-white" : "bg-white text-gray-700"
      } hover:bg-blue-100 transition duration-200`;

      btn.addEventListener("click", () => {
        currentPage = i;
        renderCards(currentPage, dataSet);
        renderPagination(dataSet);
      });

      pagination.appendChild(btn);
    }
  }

  searchInput.addEventListener("input", (e) => {
    const query = e.target.value.toLowerCase();
    filteredData = testData.filter((test) =>
      `${test.title} ${test.params} ${test.delivery}`.toLowerCase().includes(query)
    );
    currentPage = 1;
    renderCards(currentPage, filteredData);
    renderPagination(filteredData);
  });

  // Initial Load
  renderCards(currentPage, filteredData);
  renderPagination(filteredData);
</script>

