  <!-- ✅ Static Cards Script -->
  <script>
    const container = document.getElementById("card-container");
    const searchInput = document.getElementById("search-input");
    const allTests = Object.entries(testData);

    function renderCards(filteredTests) {
      container.innerHTML = "";

      if (filteredTests.length === 0) {
        container.innerHTML = `<p class="text-red-500 text-center col-span-full">No test found</p>`;
        return;
      }

      filteredTests.forEach(([key, test]) => {
        const card = document.createElement("div");
        card.className = "bg-white w-full rounded-xl shadow-lg p-4 border border-pink-200";

        card.innerHTML = `
          <div class="flex justify-between items-start">
            <div class="w-12 h-12">
              <img src="${test.icon}" class="w-full h-full object-contain" />
            </div>
            <div class="flex items-center gap-1 bg-green-100 text-green-700 px-2 py-1 rounded-full text-xs font-medium">
              <span>SAFE</span>
            </div>
          </div>

          <div class="mt-4 space-y-1 text-sm">
            <h3 class="font-semibold text-gray-800 text-xl">${test.title}</h3>
            <p class="text-gray-600 bg-pink-400 text-black text-center rounded-full py-1 px-3 w-max">Home Collection Available</p>
            <p class="text-gray-600">Report delivery - <span class="text-blue-600">${test.delivery}</span></p>
            <p>${test.fastingRequired ? "Fasting state required" : "No fasting required"}</p>
            <p>${test.parametersCovered} Parameter(s) Cover</p>
          </div>

          <div class="mt-3 flex items-center gap-3">
            <p class="text-lg font-bold">${test.price}</p>
            <p class="line-through text-gray-400">${test.oldPrice}</p>
          </div>

          <div class="mt-4 flex gap-4">
            <a href="/test-details?id=${key}" class="flex-1">
              <button class="w-full bg-blue-600 text-white py-2 rounded-md text-sm hover:bg-blue-700 transition">
                View Details
              </button>
            </a>
            <button class="flex-1 bg-gray-100 border border-gray-400 text-gray-800 py-2 rounded-md text-sm hover:bg-gray-200 transition">
              Add to Cart
            </button>
          </div>
        `;

        container.appendChild(card);
      });
    }

    searchInput.addEventListener("input", (e) => {
      const query = e.target.value.toLowerCase();
      const filtered = allTests.filter(([key, test]) => {
        const values = Object.values(test).join(" ").toLowerCase();
        return values.includes(query) || key.toLowerCase().includes(query);
      });
      renderCards(filtered);
    });

    renderCards(allTests);
  </script>

